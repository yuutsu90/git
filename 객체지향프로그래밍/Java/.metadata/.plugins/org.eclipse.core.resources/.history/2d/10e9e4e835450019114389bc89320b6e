package kr.ac.kookmin.cs.oop.ch1;

public class Hello {

	public static int sum(int n, int m) {
		return n + m;
	}
	
	public static void main(String[] args) {
		int i = 20;
		int s;
		char a;
	}
	
	
	
	public static void main(String[] args) {
		s = sum(i,10);
		int s;
		char a;
		
		s = sum(i,10);
		a = '?';
		System.out.println(a);
		System.out.println("Hello");
		System.out.println(s);

	}

}
